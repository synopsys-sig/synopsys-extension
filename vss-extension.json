{
  "manifestVersion": 1,
  "id": "synopsys-security-scan",
  "name": "Synopsys Security Scan",
  "version": "1.1.0",
  "publisher": "synopsys-security-scan",
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Polaris",
    "Synopsys",
